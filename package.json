{
    "name": "lnreader-plugins",
    "version": "1.0.0",
    "description": "Plugins repo for LNReader",
    "main": "index.js",
    "scripts": {
        "start": "tsc-watch --onSuccess \"node ./.js/index.js\"",
        "test": "exit 0",
        "json": "tsc && mv ./.js/index.js ./.js/index.js.temp && node ./.js/scripts/json_plugins.js && mv ./.js/index.js.temp ./.js/index.js && git add plugins && git add .dist && git add .js/plugins",
        "generate": "ts-node ./scripts/multisrc/generate.ts && tsc",
        "clearMulti": "ts-node ./scripts/clearMultisrc.ts",
        "less": "npx less ./test_web/static/css/index.less ./test_web/static/css/index.css"
    },
    "author": "LNReader",
    "license": "MIT",
    "dependencies": {
        "body-parser": "^1.20.2",
        "browserify": "^17.0.0",
        "cheerio": "^1.0.0-rc.10",
        "cli-color": "^2.0.3",
        "dayjs": "^1.11.4",
        "express": "^4.18.2",
        "less": "^4.2.0",
        "module-alias": "^2.2.2",
        "sanitize-html": "^2.11.0",
        "ts-node": "^10.9.1",
        "tsc-alias": "^1.8.6",
        "typescript": "^5.1.3",
        "urlencode": "^2.0.0"
    },
    "_moduleAliases": {
        "@libs": "./.js/libs",
        "@icons": "./.js/icons",
        "@plugins": "./.js/plugins",
        "@scripts": "./.js/scripts",
        "@typings": "./.js/types"
    },
    "devDependencies": {
        "@types/express": "^4.17.17",
        "@types/jquery": "^3.5.28",
        "@types/node": "^20.3.1",
        "pre-commit": "^1.2.2",
        "tsc-watch": "^6.0.4"
    },
    "pre-commit": [
        "json"
    ]
}
